<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.m2.testegps.gps">

    <TextView
        android:id="@+id/id_latitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="40dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="36dp"
        android:text="@string/carregando"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/id_longitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:text="@string/carregando"
        android:textSize="20sp"
        android:textStyle="bold"
        android:layout_below="@+id/id_latitude"
        android:layout_alignLeft="@+id/id_latitude"
        android:layout_alignStart="@+id/id_latitude" />

    <EditText
        android:gravity="center"
        android:id="@+id/edt_resposta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Resposata"
        android:layout_below="@+id/id_longitude"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="92dp"
        android:text="VOLTAR"
        android:onClick="voltar"/>


</RelativeLayout>
